    <changeSet id="1484859230_create_users_table" author="sagikazarmark">
        <sql><![CDATA[
CREATE TABLE users (
    id INT AUTO_INCREMENT NOT NULL,
    username VARCHAR(255) NOT NULL,
    encrypted_password VARCHAR(4096) NOT NULL,
    PRIMARY KEY (id),
    UNIQUE KEY (username)
);
]]></sql>
    </changeSet>
</databaseChangeLog>
